    <RootNamespace>Chatbees.Engine</RootNamespace>
    <PackageId>ChatbeeEngine</PackageId>
    <Version>0.1.0</Version>
    <Authors>atotalnoob</Authors>
    <Company>ChatBee</Company>
    <Title>Chatbee Automation Engine</Title>
    <RepositoryUrl>https://github.com/chatbee/engine</RepositoryUrl>
